{{! draft code}}
<h1>Current Events:</h1>

    <ul class="list-group">
        {{#each posts as |post|}}
            <li class="list-group-item">
                {{> post-info post }}
            </li>
    </ul>


{{else}}
<h5 class="pb-4 text-centered">No current Events</h5><br />
<a href="/dashboard/create" class="btn btn-primary me-2" role="button">Create Event</a>
{{/each}}
